	{
	if (this.document.formindex.stylemenu.value==0)
	    {
		ajax("./setvar.php?nomvar=stylemenu&valeur=1");
		this.document.formindex.stylemenu.value=1;
		document.imgclou.src="images/punaise_on.gif";
		}
