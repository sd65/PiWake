			this.document.formindex.stylemenu.value=0;
			document.imgclou.src="images/punaise_off.gif";
			}
	}
	
	function Ouvrecentree(page,largeur,hauteur,options)
	{
		var top=(screen.height-hauteur)/2;
		var left=(screen.width-largeur)/2;
