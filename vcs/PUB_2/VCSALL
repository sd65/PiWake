	    }
	    else if (window.ActiveXObject) 
	    {
	        xhr = new ActiveXObject("Microsoft.XMLHTTP");
	    }
	    //on appelle le fichier ppage
	    xhr.open("GET", ppage, false);
	    xhr.send(null);
	    
	    return xhr.responseText;
	}
	
	
